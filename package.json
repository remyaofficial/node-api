  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "seed": "ts-node -r tsconfig-paths/register src/core/modules/seeder/seed.ts",
    "pm2:start": "node ./node_modules/pm2/bin/pm2 start ecosystem.config.js",
    "pm2:stop": "node ./node_modules/pm2/bin/pm2 stop ecosystem.config.js",
    "pm2:reload": "node ./node_modules/pm2/bin/pm2 reload ecosystem.config.js",
    "pm2:log": "node ./node_modules/pm2/bin/pm2 logs",
    "prod": "npm-run-all build pm2:stop pm2:start pm2:log",
    "prod:start": "npm-run-all pm2:stop pm2:start pm2:log",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
